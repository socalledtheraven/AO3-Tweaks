{
    "description": "fixes bookmark links to go to the last page instead, and adds a rekudos thing",
    "manifest_version": 2,
    "name": "Ao3 utils",
    "version": "1.0.2",
    "content_scripts": [
		{
	    	"js": ["inWork.js"],
	    	"matches": [
	        	"*://archiveofourown.org/works*"
	    	]
    	}, {
			"js": ["bookmarks.js"],
	    	"matches": [
				"*://archiveofourown.org/bookmarks*sort_column%5D=bookmarkable_date*"
	    	]
		}
	],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": "favicon.png"
	}
}